<favorites xmlns:launcher="http://schemas.android.com/apk/res-auto/com.android.launcher3">

    <!-- Hotseat -->
    <!--<include launcher:workspace="@xml/dw_phone_hotseat" />-->

	<!-- Digital Clock Widget -->
    <appwidget
        launcher:container="-100"
        launcher:packageName="com.android.deskclock"
        launcher:className="com.android.alarmclock.DigitalAppWidgetProvider"
        launcher:screen="0"
        launcher:spanX="4"
        launcher:spanY="2"
        launcher:x="1"
        launcher:y="-3"/>

   <!-- Settings App -->
    <resolve
        launcher:container="-100"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="-1">
        <favorite
	        launcher:className="com.android.settings.Settings"
	        launcher:packageName="com.android.settings"/>
    </resolve>

	<!-- chrome App -->
    <resolve
        launcher:container="-100"
        launcher:screen="0"
        launcher:x="1"
        launcher:y="-1">
        <favorite
        	launcher:className="com.google.android.apps.chrome.Main"
	        launcher:packageName="com.android.chrome"/>
    </resolve>

   	<!-- File App -->
    <resolve
        launcher:container="-100"
        launcher:screen="0"
        launcher:x="2"
        launcher:y="-1">
        <favorite
	        launcher:className="com.android.documentsui.LauncherActivity"
	        launcher:packageName="com.android.documentsui"/>
    </resolve>

   	<!-- Gamepadtest App -->
    <resolve
        launcher:container="-100"
        launcher:screen="0"
        launcher:x="3"
        launcher:y="-1">
        <favorite
	        launcher:className="com.android.vending.AssetBrowserActivity"
	        launcher:packageName="com.android.vending"/>
    </resolve>

	<!-- Music App -->
    <resolve
        launcher:container="-100"
        launcher:screen="0"
        launcher:x="4"
        launcher:y="-1">
        <favorite
	        launcher:className="com.kabouzeid.gramophone.ui.activities.MainActivity"
	        launcher:packageName="com.odin.odinmusic"/>
    </resolve>

	<!-- Gallery App -->
    <resolve
        launcher:container="-100"
        launcher:screen="0"
        launcher:x="5"
        launcher:y="-1">
        <favorite
	        launcher:className="com.android.gallery3d.app.GalleryActivity"
	        launcher:packageName="com.android.gallery3d"/>
    </resolve>

